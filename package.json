{
  "name": "flux-react-router-todoapp",
  "description": "flux-react-router-todoapp",
  "private": true,
  "scripts": {
    "start": "jade -o build-dev -P -w src/template/index.jade | webpack-dev-server --config ./config/webpack.config.babel.js",
    "api:mock": "json-server -d 400 --watch db/db.json",
    "clean:build": "if [ -d build ]; then rm -rf build; fi",
    "jade:build": "jade -o build src/template/index.jade",
    "build": "npm_config_production=true && npm ru clean:build && webpack --config ./config/webpack.config.babel.js && MAIN_SCRIPT=/p/assets/$(basename `ls build/assets/app-*.js`) npm ru jade:build"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "flux": "^2.1.1",
    "history": "^2.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "whatwg-fetch": "github:sugarshin/fetch#84b2b2328d58252df63ce31f747e51c4b7e8742b"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.23.1",
    "css-mqpacker": "^4.0.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^3.16.1",
    "jade": "^1.11.0",
    "json-server": "^0.8.8",
    "postcss-loader": "^0.8.1",
    "style-loader": "^0.13.0",
    "stylint": "^1.3.6",
    "stylint-loader": "^1.0.0",
    "stylint-stylish": "^1.2.0",
    "stylus-loader": "^1.5.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
